  "presentationEntries": [
    {
      "displayName": "Content",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Core Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "name",
          "visibleOnAdd": true
        },
        {
          "id": "entityId",
          "visibleOnAdd": true
        },
        {
          "id": "datePosted"
        },
        {
          "id": "slug"
        },
        {
          "id": "c_coverPhoto"
        },
        {
          "id": "c_body"
        },
        {
          "id": "c_description"
        },
        {
          "id": "c_metaDescription"
        },
        {
          "id": "c_keywords"
        }
      ]
    },
    {
      "displayName": "Neighborhood Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "c_neighborhoodName"
        },
        {
          "id": "c_neighborhoodNickname"
        },
        {
          "id": "c_neighborhoodDescription"
        },
        {
          "id": "c_landmarks"
        },
        {
          "id": "c_restaurants"
        }
      ]
    },
    {
      "displayName": "Other",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Internal Use Only",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "folder"
        },
        {
          "id": "labels"
        }
      ]
    }
  ]
}